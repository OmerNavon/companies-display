{
  "name": "companies-display",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:server\" \"npm:dev:web\"",
    "dev:web": "next dev",
    "dev:server": "tsx --tsconfig tsconfig.server.json server/index.ts",
    "build": "next build",
    "start": "concurrently -k \"npm:dev:server\" \"next start\"",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.6.0",
    "firebase": "^11.0.2",
    "next": "16.0.7",
    "openai": "^4.77.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "@mui/material": "^6.1.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "react-icons": "^5.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tsx": "^4.19.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
